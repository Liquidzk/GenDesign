<view class="content"><form data-event-opts="{{[['submit',[['sub',['$event']]]]]}}" onSubmit="__e"><view class="list"><view class="list-title">健康信息</view><view class="list-item"><text>姓名<text class="color-red"></text>：</text><input value="{{name}}" placeholder="请输入人员姓名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" onInput="__e"/></view><view class="list-item"><text>电话号：</text><input value="{{phone}}" placeholder="请输入电话号" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" onInput="__e"/></view><view class="list-item"><text>进出：</text><radio-group data-event-opts="{{[['change',[['healthyChange',['$event']]]]]}}" onChange="__e"><label><radio value="1"></radio><text>进</text><radio value="0"></radio><text>出</text></label></radio-group></view></view><view class="list"><button form-type="submit">提交信息</button></view></form></view>