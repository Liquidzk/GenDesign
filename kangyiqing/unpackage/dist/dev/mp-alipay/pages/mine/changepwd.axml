<view class="content"><form data-event-opts="{{[['submit',[['sub',['$event']]]]]}}" onSubmit="__e"><view class="list"><view class="list-item"><text>旧密码：</text><input value="{{passwd}}" placeholder="请输入旧密码" data-event-opts="{{[['input',[['__set_model',['','passwd','$event',[]]]]]]}}" onInput="__e"/></view><view class="list-item"><text>新密码：</text><input value="{{newpasswd}}" placeholder="请输入新密码" data-event-opts="{{[['input',[['__set_model',['','newpasswd','$event',[]]]]]]}}" onInput="__e"/></view></view><view class="list"><button form-type="submit">确定</button></view></form></view>